{
  "name": "gatsby-test",
  "description": "Gatsby Test",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Paul Vallis",
  "repository": {
    "type": "git",
    "url": "https://github.com/vazerthon/gatsby-test"
  },
  "scripts": {
    "cleancache": "rimraf .cache/*",
    "build": "npm run cleancache && gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run cleancache && npm run develop",
    "format": "prettier --write '**/*.js'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@wapps/gatsby-plugin-material-ui": "^1.0.7",
    "ajv": "^6.5.4",
    "babel-plugin-styled-components": "^1.8.0",
    "classnames": "^2.2.6",
    "gatsby": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-json": "^2.1.4",
    "jss": "^9.8.7",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-slick": "^0.23.1",
    "rimraf": "^2.6.2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2"
  }
}
